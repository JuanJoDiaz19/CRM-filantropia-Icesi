{
    "version":2,
    "builds":[
        {
            "src":"CRM_filantropia_ICESI/wsgi.py",
            "use":"@vercel/python",
            "config":{
                "maxLambdaSize":"15mb",
                "runtime":"python3.9"
            }
        },
        {
            "src": "build.sh",
            "use": "@vercel/static-build",
            "config": {
              "distDir": "staticfiles_build"
            }
        }
    ],
    "routes":[
        {
            "src": "/static/(.*)",
            "dest": "/static/$1"
        },
        {
            "src":"/(.*)",
            "dest":"CRM_filantropia_ICESI/wsgi.py"
        }
    ]
}